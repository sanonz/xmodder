{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Browser",
  "extends": "./base.json",
  "compilerOptions": {
    "declarationMap": true,
    "incremental": false,
    "lib": ["es2022", "DOM", "DOM.Iterable"],
    "moduleDetection": "force",
    "noUncheckedIndexedAccess": true,
    "resolveJsonModule": true,
    "strict": true,
    "target": "ES2023"
  }
}
